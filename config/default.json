{
    "LoggerW" : {
        "logFile":{
            "location": "/logs",
            "fileNameFormat":"[TS].log",
            "fileNameTimeStampFormat":"YYYYMMDD",
            "enableOriginFileLog": true,
            "enableFileLog": true,
            "enableConsoleLog": true
        },
        "express": {
            "enableRequestLog": true,
            "enableResponseLog": true
        },
        "ids":{
            "request": {
                "enable": true,
                "header": "x-request"
            },
            "correlation": {
                "enable": true,
                "header": "x-request"
            }
        }
    }
}